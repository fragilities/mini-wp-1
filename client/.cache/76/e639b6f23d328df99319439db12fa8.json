istory',\n   base: process.env.BASE_URL,\n   routes\n})\n\nconst whitelist = [\n   'register',\n   'login'\n]\n\nrouter.beforeEach((to, from, next) => {\n   if(!whitelist.includes(to.name) && !localStorage.token) {\n      Swal.fire({\n         position: 'center',\n         icon: 'info',\n         title: 'You are not logged in, redirecting...',\n         showConfirmButton: false,\n         timer: 1000\n       })\n\n      next({name: 'login'})\n   }\n   else next()\n})\n\nexport default router"},"lineCount":null}},"error":null,"hash":"8a2757408a3bdb64e3cda92b2e1516e0","cacheData":{"env":{}}}